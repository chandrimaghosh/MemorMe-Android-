
<FrameLayout
   xmlns:android="http://schemas.android.com/apk/res/android"
   xmlns:app="http://schemas.android.com/apk/res-auto"
   xmlns:tools="http://schemas.android.com/tools"
   android:layout_width="match_parent"
   android:layout_height="match_parent"
   android:clipChildren="false"
   tools:context=".tictactoe.MainActivity">

   <syedamanalam.madcourse.neu.edu.numad17s_syedamanalam.tictactoe.ScrollingView
      android:id="@+id/main_background"
      android:layout_width="match_parent"
      android:layout_height="match_parent"
      app:scrollingDrawable="@drawable/xando_background"/>

   <fragment
      android:id="@+id/main_fragment"
      class="syedamanalam.madcourse.neu.edu.numad17s_syedamanalam.tictactoe.MainFragment"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_gravity="center"
      tools:layout="@layout/fragment_main"/>
</FrameLayout>
