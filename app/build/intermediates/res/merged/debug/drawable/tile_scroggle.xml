<?xml version="1.0" encoding="utf-8"?>

<level-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <!-- The main tile where we will write letters from a to z from java files-->
    <item
        android:drawable="@drawable/buttonssmallsquare"
        android:maxLevel="0" />

    <!-- Inactive Gray Tile which is already used-->
    <item
        android:drawable="@drawable/tile_empty"
        android:maxLevel="1" />

    <!--Which 3*3 grid is available will get an green color-->
    <item
        android:drawable="@drawable/tile_available"
        android:maxLevel="2" />

</level-list>